{
	"name": "rsvpizza",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"debug": "NODE_OPTIONS='--inspect' next dev",
		"postinstall": "prisma generate",
		"lint": "next lint",
		"format": "prettier --write .",
		"lint:ts": "tsc --noEmit",
		"check": "npm run lint && npm run lint:ts && npm run format",
		"prepare": "husky install",
		"start": "next start",
		"angry": "npx rimraf node_modules package-lock.json .next && npm install",
		"full-angry": "npm run angry && npm run check && npm run build && npm run dev"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx}": [
			"prettier --write",
			"eslint --fix"
		],
		"*": "tsc -p tsconfig.json --noEmit --skipLibCheck"
	},
	"dependencies": {
		"@chakra-ui/icons": "^2.0.19",
		"@chakra-ui/react": "^2.5.5",
		"@clerk/nextjs": "^4.17.1",
		"@emotion/react": "^11.10.6",
		"@emotion/styled": "^11.10.6",
		"@prisma/client": "^4.11.0",
		"@trpc/client": "^10.25.0",
		"@trpc/next": "^10.31.0",
		"@trpc/react-query": "^10.25.0",
		"@trpc/server": "^10.25.0",
		"axios": "^1.4.0",
		"dayjs": "^1.11.7",
		"framer-motion": "^10.10.0",
		"hot": "^0.0.7",
		"next": "^13.2.4",
		"react": "^18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.43.9",
		"react-hot-toast": "^2.4.1",
		"react-query": "^3.39.3",
		"react-router-dom": "^6.11.1",
		"superjson": "1.12.2",
		"swr": "^2.1.2",
		"toast": "^0.5.4",
		"tree": "^0.1.3",
		"trpc": "^0.11.3",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@types/eslint": "^8.21.3",
		"@types/node": "^18.16.5",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@types/react-router-dom": "^5.3.3",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"eslint": "^8.36.0",
		"eslint-config-next": "^13.2.4",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.0",
		"prettier": "^2.8.7",
		"prisma": "^4.13.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	},
	"ct3aMetadata": {
		"initVersion": "7.10.2"
	},
	"prisma": {
		"seed": "node --loader ts-node/esm prisma/seed.ts"
	}
}
